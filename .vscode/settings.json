{
  "typescript.tsdk": "node_modules/typescript/lib",
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit",
    "quickfix.biome": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "i18n-ally.localesPaths": ["./messages"], // E.g. "./messages"
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "ko",
  "i18n-ally.displayLanguage": "ko",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[less]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "files.associations": {
    "*.css": "css"
  },
  "scss.format.spaceAroundSelectorSeparator": true,
  "css.format.spaceAroundSelectorSeparator": true,
  "less.format.spaceAroundSelectorSeparator": true,
  "css.format.preserveNewLines": true,
  "scss.format.preserveNewLines": true,
  "less.format.preserveNewLines": true,
  "css.format.maxPreserveNewLines": 2,
  "scss.format.maxPreserveNewLines": 2,
  "less.format.maxPreserveNewLines": 2
}
